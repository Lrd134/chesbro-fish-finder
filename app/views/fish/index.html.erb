<p id="notice"><%= notice %></p>

<h1>Fish</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fish.each do |fish| %>
      <tr>
        <td><%=fish.title%></td>
        <td><%=fish.category.name.capitalize%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
