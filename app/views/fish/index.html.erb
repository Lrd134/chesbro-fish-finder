<p id="notice"><%= notice %></p>

<h1>Fish</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fish.each do |fish| %>
      <tr>
        <td><%=link_to fish.title, fish_path(fish.slug, fish.category.slug)%></td>
        <td><%=fish.category.name.capitalize%></td>
        <td><%=link_to "Edit #{fish.title}", edit_fish_path(fish.slug, fish.category.slug)%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
